<script>

    function getBranches() {
        var branch_name_list = ["Koramangala", "Indiranagar", "Sarjapur Road"];
        var branch_map_link_list = ["https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3888.593334871297!2d77.61731551482157!3d12.93383849088066!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bae144545fac74b%3A0x11876d95df575c1a!2sKia+Kaha+MMA+Koramangala!5e0!3m2!1sen!2sin!4v1541415537459", "https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3888.0993938388942!2d77.63950071385666!3d12.965491318522654!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bae141c69e24847%3A0x88aa1172394d3b8f!2sKia+Kaha+MMA+Indiranagar!5e0!3m2!1sen!2sin!4v1541415995363", "https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3888.875987026742!2d77.67074331385612!3d12.915691219599475!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bae137269c32c73%3A0xd54466d094077e78!2sKia+Kaha+MMA+Sarjapur+Road!5e0!3m2!1sen!2sin!4v1541416253903"];


        var div_location = '';
        for (var L = 0; L < branch_name_list.length; L++) {


            var margin_left = 'margin-left: 15px;';
            if (L == 0) {
                margin_left = 'margin-left: 10px;';
            }

            var custom_style = '';
            if (jQuery(window).width() < 500) {//FOR MOBILE
                custom_style = 'style="display: inline-block; width:30%; ' + margin_left + '"';
            } else {
                custom_style = 'class="col-md-4"';
            }

            div_location = div_location
                + '<div ' + custom_style + '>'
                + '<div class="card card-primary">'
                    + '<div class="card-body">'
                        + '<h3 style="color:#991412;text-align: center;font-size: 15px;">' + branch_name_list[L]

                        + '</h3>'

                        + '<p style="text-align: center;"><iframe id="map1_iframe" style="width: 100%;" src="' + branch_map_link_list[L] + '"width="150" height="100" frameborder="0" allowfullscreen="allowfullscreen" data-mce-fragment="1"></iframe></p>'

                    + '</div>'
                + '</div>'
                + '</div>';
        }

        if (jQuery(window).width() < 500) {//FOR MOBILE
            div_location = '<div style="overflow: auto;white-space: nowrap;">' + div_location + '</div>';
        }

        $('#div_location').html(div_location);


    }


</script>


<script>

    var prod_programs_size = 0;
    var prod_packages_size = 0;

    function getProductDetails(prod_category) {

        var inputJson = {
            prod_category: prod_category
        };
        console.log("getProductDetails Request : ", JSON.stringify(inputJson));
        jQuery.ajax({
            type: "POST",
            url: '/FrontHome/getProductDetails',
            dataType: 'json',
            data: JSON.stringify(inputJson),
            contentType: "application/json; charset=utf-8",
            success: function (res) {
                console.log("getProductDetails Response : ", JSON.stringify(res));
                $('#btn_submit').prop('disabled', false);
                $(".overlay").hide();

                if (res.is_success) {

                    if (prod_category == "PROGRAMS") {
                        var program_detials = "";
                        var a = 2;
                        prod_programs_size = res.details_list.length;
                        for (var i = 0; i < res.details_list.length; i++) {
                            var prod = res.details_list[i];
                            var _id = "prod" + i;
                            var url_padding = "&&product_name= " + prod.prod_name + removeTags(prod.prod_description);
                            program_detials = program_detials

                               + '<div id="' + _id + '" class="col-lg-3 col-md-4 col-xs-6" >'
                                + '<a href="/ProductDetails/ProductDetails?product_id=' + prod.prod_guid + url_padding + '" style="width:100%;">'
                                  + '<img src="/Uploads/Product_images/' + prod.prod_image + '" alt="Image" style="width:100%;padding-bottom: 15px;">'
                                   //+ '<center> <h5 style="color:#000000;">' + prod.prod_name + ' </h5> </center>'
                                + ' </a>'
                               + '</div>';

                        }
                        $('#div_programs_products').html(program_detials);
                        checkWidth();
                        getProductDetails('PACKAGES');
                    } else if (prod_category == "PACKAGES") {
                        var package_detials = "";
                        prod_packages_size = res.details_list.length;
                        for (var i = 0; i < res.details_list.length; i++) {
                            var prod = res.details_list[i];
                            //var id = "prod_pkg" + i; class="col-lg-3 col-md-3 col-xs-6"
                            var margin_left = 'margin-left: 10px;';
                            if (i == 0) {
                                margin_left = 'margin-left: 8px;';
                            }


                            var custom_style = '';
                            if (jQuery(window).width() < 500) {//FOR MOBILE
                                custom_style = 'style="display: inline-block; width:40%; ' + margin_left + '"';
                            } else {
                                custom_style = 'class="col-lg-3 col-md-3 col-xs-6"';
                            }

                            var url_padding = "&&product_name= " + prod.prod_name + removeTags(prod.prod_description);

                            package_detials = package_detials

                               + '<div ' + custom_style + '>'
                                + '<a href="/ProductDetails/ProductDetails?product_id=' + prod.prod_guid + url_padding+ '" style="">'
                                  + '<img src="/Uploads/Product_images/' + prod.prod_image + '" alt="Image" style="width:100%;padding-bottom: 15px;">'
                                        + ' <table style="width:100%;">'
                                              + '<tr>'
                                                   + ' <td style="text-align: left;color:#000000;">Price</td>'
                                                      + '<td style="text-align: right; color:#d3d3d3;"><strike style="color: black;">&#8377; ' + prod.prod_mrp + '</strike></td>'
                                                       + ' </tr>'
                                                       + '<tr>'
                                                    + ' <td style="text-align: left;color:#000000;">Offer Price</td>'
                                                      + '<td style="text-align: right; color:#FF0000;">&#8377; ' + prod.prod_price + '</td>'
                                                      + '</tr>'
                                                          + '</table>'
                                + ' </a>'
                               + '</div>'


                        }
                        if (jQuery(window).width() < 500) {//FOR MOBILE
                            package_detials = '<div style="overflow: auto;white-space: nowrap;">' + package_detials + '</div>'
                           + ' <div class="col-lg-12 col-md-12 col-xs-12">'
                           + '  <a style="float:right;"><i class="fas fa-arrow-circle-right"></i></a>'

     + '</div>'
                        }

                        $('#div_package_products').html(package_detials);
                        checkWidth();

                    }

                } else {
                    alert(res.msg);
                }

            },
            failure: function (data) {
                alert("Network error occurred.");
                $(".overlay").hide();

            },
            error: function (jqXHR, exception) {
                if (jqXHR.status === 0) {
                    alert('Please Check Your Internet Connection.');
                } else if (jqXHR.status == 404) {
                    alert('Requested page not found. [404]');
                } else if (jqXHR.status == 500) {
                    alert('Internal Server Error [500].');
                } else if (exception === 'parsererror') {
                    alert('Requested JSON parse failed.');
                } else if (exception === 'timeout') {
                    alert('Time out error.');
                } else if (exception === 'abort') {
                    alert('Ajax request aborted.');
                } else {
                    alert('Uncaught Error.\n' + jqXHR.responseText);
                }
                $(".overlay").hide();
            }

        });

    }


</script>


<script>
    function removeTags(str) {
        if ((str === null) || (str === ''))
            return false;
        else
            str = str.toString();

        // Regular expression to identify HTML tags in 
        // the input string. Replacing the identified 
        // HTML tag with a null string.
        return str.replace(/(<([^>]+)>)/ig, '');
    }
</script>

