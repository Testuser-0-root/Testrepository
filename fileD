<script>

    var final_prod_id = "";
    var final_date_ofbook = "";
    var final_tt_guid = "";
    var final_amount = "";

</script>











<script>
    function initiatePayment() {
        var product_id = final_prod_id;
        var amount = final_amount;
        var tt_guid = final_tt_guid;
        var start_date = final_date_ofbook;



        var branch_guid = "";
        if ($('#sel_branch').length) {
            branch_guid = $('#sel_branch').val();
            if (branch_guid == "") {
                branch_guid = $('#sel_center').val();
            }
        } else {
            branch_guid = $('#sel_center').val();

        }

        var cust_guid = '';
        if (cust_guid == "") {
            showLogin();
            return;
        }

        if (branch_guid == "") {
            alert("Select Center");
            return;
        }

        //var terms = document.getElementById("Check1");
        //if (!terms.checked) {
        //    alert('You must agree to the terms first1.');
        //    return;
        //}



        var cust_name = '';
        var cust_mobile_number = '';
        var cust_email = '';
        var tt_guid = "";


        var inputJson = {
            prod_guid: product_id,
            cust_guid: cust_guid,
            tt_guid: tt_guid,
            amount: amount,
            start_date: start_date,
            branch_guid: branch_guid
        };
        console.log("initiatePayment input : ", JSON.stringify(inputJson));
        $('#txt_paymentbutton').prop('disabled', true);
        $(".overlay").show();

        jQuery.ajax({
            type: "POST",
            url: '/ProductDetails/initiatePayment',  
            contentType: "application/json",
            dataType: 'json',
            data: JSON.stringify(inputJson),
            success: function (res) {
                console.log("initiatePayment output : ", JSON.stringify(res));
                if (res.is_success) {
                    var pm = res.payu_money_details[0];
                    $('#amount').val(amount);
                    $('#is_website').val("YES");
                    $('#payment_for').val("PRODUCT");
                    $('#uid').val(cust_guid);
                    $('#firstname').val(cust_name);
                    $('#lastname').val(cust_name);
                    $('#email').val(cust_email);
                    $('#phone').val(cust_mobile_number);
                    $('#productinfo').val(pm.pm_product_info);
                    $('#merchant_key').val(pm.pm_merchant_key);
                    $('#salt').val(pm.pm_salt);
                    $('#merchant_id').val(pm.pm_merchant_id);
                    $('#payu_base_url').val(pm.pm_payu_base_url);
                    $('#surl').val(pm.pm_surl);
                    $('#furl').val(pm.pm_furl);
                    $('#txnid').val(pm.pm_trans_id);

                    var payuForm = document.forms.payuForm;
                    payuForm.submit();

                } else {
                    alert(res.msg);
                }

            },
            failure: function (data) {
                alert("Network error occurred.");
                $('#txt_paymentbutton').prop('disabled', false);
                $(".overlay").hide();
            },
            error: function (jqXHR, exception) {
                if (jqXHR.status === 0) {
                    alert('Please Check Your Internet Connection.');
                } else if (jqXHR.status == 404) {
                    alert('Requested page not found. [404]');
                } else if (jqXHR.status == 500) {
                    alert('Internal Server Error [500].');
                } else if (exception === 'parsererror') {
                    alert('Requested JSON parse failed.');
                } else if (exception === 'timeout') {
                    alert('Time out error.');
                } else if (exception === 'abort') {
                    alert('Ajax request aborted.');
                } else {
                    alert('Uncaught Error.\n' + jqXHR.responseText);
                }
                $('#txt_paymentbutton').prop('disabled', false);
                $(".overlay").hide();
            }

        });

    }
</script>



<script>
    $(function () {
        checkScreenSizeInFrontEnd();
    });

    function checkScreenSizeInFrontEnd() {

        if (jQuery(window).width() < 340) {
            $('#main_body').addClass('main_body_style');
        }

        if (jQuery(window).width() < 500) {

            $('#a_img_logo').addClass('logo_size_mobile');
            $('#a_menu_free_trial').addClass('fixed_menu_mobile');
            $('#li_customer_login').addClass('menu_sign_in_div_mobile');


            $('#a_menu_programs').addClass('menu_text_font');
            $('#a_menu_time_table').addClass('menu_text_font');
            $('#a_menu_sign_in').addClass('menu_text_font');
        } else {
            $('#a_img_logo').addClass('logo_size');
            $('#a_menu_free_trial').addClass('fixed_menu');
            $('#li_customer_login').addClass('menu_sign_in_div');
        }
    }

</script>





