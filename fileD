    <script>

    function showLogin() {
        $('#modal-login').show();
        $('#modal-login').addClass('show');
    }

    function hideLogin() {
        $('#modal-login').hide();
        $('#modal-login').removeClass('show');
    }

    function openCenterTimeTablePopup() {

        var class_customer_guid = '';

            var inputJson = {
                class_customer_guid: class_customer_guid
            };
            console.log("ActiveMemberLastClassBookedWithoutRateCustomerID Request : ", JSON.stringify(inputJson));
            jQuery.ajax({
                type: "POST",
                url: '/BookTheClass/ActiveMemberLastClassBookedWithoutRateCustomerID',
                dataType: 'json',
                data: JSON.stringify(inputJson),
                contentType: "application/json; charset=utf-8",
                success: function (res) {
                    console.log("ActiveMemberClassBookingListByCustomerID : ", JSON.stringify(res));
                    if (res.is_success) {
                        if (res.msg != "Not Registered") {
                             if (res.customer_class_booking_list.length != 0) {
                                 for (var s = 0; s < res.customer_class_booking_list.length; s++) {
                                     var st = res.customer_class_booking_list[s];

                                     if (st.class_is_rated === false) {
                                         // showWeekNameAndDate('2024-11-18');
                                         //showWeekNameAndDate(st.class_booking_date);

                                         showWeekNameAndDate();
                                         document.getElementById("div_center_time_table").style.display = "block";
                                         $('#msgRateNow').show();
                                         // $('#ClassBooking').hide();

                                     } else {

                                         showWeekNameAndDate();
                                     }

                                 }
                                  }
                                else {
                                    showWeekNameAndDate();
                                }

                        } else {
                            showWeekNameAndDate();
                            document.getElementById("div_center_time_table").style.display = "block";
                        }
                    }

                },
                failure: function (data) {
                    alert("Network error occurred.");
                    $(".overlay").hide();

                },
                error: function (jqXHR, exception) {
                    if (jqXHR.status === 0) {
                        alert('Please Check Your Internet Connection.');
                    } else if (jqXHR.status == 404) {
                        alert('Requested page not found. [404]');
                    } else if (jqXHR.status == 500) {
                        alert('Internal Server Error [500].');
                    } else if (exception === 'parsererror') {
                        alert('Requested JSON parse failed.');
                    } else if (exception === 'timeout') {
                        alert('Time out error.');
                    } else if (exception === 'abort') {
                        alert('Ajax request aborted.');
                    } else {
                        alert('Uncaught Error.\n' + jqXHR.responseText);
                    }
                    $(".overlay").hide();
                }

            });

    }

        //function openCenterTimeTablePopup() {

        //    //showWeekNameAndDate('16/11/2024');
        //    showWeekNameAndDate()
        //   // showWeekNameAndDate('2024-11-16');
        //    document.getElementById("div_center_time_table").style.display = "block";
        //}


    function closeCenterTimeTablePopup() {
        document.getElementById("div_center_time_table").style.display = "none";
    }

    function showClassBookedPopup1() {
        var cust_guid = '';
        if (cust_guid == "") {
            alert("Please Register First")
        }
        }
    </script>

    <script>

            /* Book The Class start here */
        function showBookTheClassPopup1(mem_branch_id, prod_guid, class_name, start_time, end_time, date_of_book, tt_guid) {
            alert("hidd");
    var cust_guid = '';
    if (cust_guid == "") {

        showLogin();
        return;
    }
    var mem_cust_guid = '';
    var inputJson = {
        mem_branch_id: mem_branch_id,
        prod_guid: prod_guid,
        class_name: class_name,
        start_time: start_time,
        end_time: end_time,
        mem_cust_guid: mem_cust_guid,
        tt_guid: tt_guid,
        date_of_book: date_of_book

    };

            console.log("showBookTheClassPopup input : ", JSON.stringify(inputJson));
    $('#btn_submit').prop('disabled', true);
    $(".overlay").show();
    jQuery.ajax({
        type: "POST",
        url: '/ProductDetails/BookTheClassPopup',
            contentType: "application/json",
            dataType: 'json',
            data: JSON.stringify(inputJson),
            success: function (res) {
                $('#btn_submit').prop('disabled', false);
                console.log("showBookTheClassPopup result : ", JSON.stringify(res));
                if (res.is_success) {

                    //  $('#book_Program').attr('hidden', true);;

                    swal({
                        title: "Class Booked!!!",
                        text: "Your class has been successfully booked!",
                        type: "success",
                        confirmButtonText: "OK"
                    }, function (isConfirm) {
                        if (isConfirm) {
                            window.location = "/FrontHome/FrontHome";
                        }
                    });

                } else {
                    if (!res.is_it_free) {

                        $('#a_menu_free_trial').hide();

                        final_prod_id = prod_guid;
                        final_date_ofbook = date_of_book;
                        final_tt_guid = tt_guid;
                        final_amount = "500";
                        console.log("final_prod_id: " + final_prod_id);
                        console.log("final_tt_guid: " + final_tt_guid);

                        //initiatePayment();
                        alert("Success");
                        window.location.reload();
                    }

                }

            },
            failure: function (data) {
                alert("Network error occurred.");
                $('#btn_submit').prop('disabled', false);
                $(".overlay").hide();
            },
            error: function (jqXHR, exception) {
                if (jqXHR.status === 0) {
                    alert('Please Check Your Internet Connection.');
                } else if (jqXHR.status == 404) {
                    alert('Requested page not found. [404]');
                } else if (jqXHR.status == 500) {
                    alert('Internal Server Error [500].');
                } else if (exception === 'parsererror') {
                    alert('Requested JSON parse failed.');
                } else if (exception === 'timeout') {
                    alert('Time out error.');
                } else if (exception === 'abort') {
                    alert('Ajax request aborted.');
                } else {
                    alert('Uncaught Error.\n' + jqXHR.responseText);
                }
                $('#btn_login').prop('disabled', false);
                $(".overlay").hide();
            }

        });


    }
            /* Book The Class start end */

    </script>

