    <script>
        // Get last Booked Classes
    function GetLastBookedClass() {
        var class_customer_guid = '';

        var inputJson = {
            class_customer_guid: class_customer_guid
        };
        console.log("ActiveMemberClassBookingListByCustomerID Request : ", JSON.stringify(inputJson));
        $('#img_gif_loading_students_list').show();
        jQuery.ajax({
            type: "POST",
            url:'/FrontHome/ActiveMemberLastBookedClassByCustomerID',
            dataType: 'json',
            data: JSON.stringify(inputJson),
            contentType: "application/json; charset=utf-8",
            success: function (res) {
                console.log("ActiveMemberClassBookingListByCustomerID : ", JSON.stringify(res));
            $('#btn_submit').prop('disabled', false);
            $('#img_gif_loading_students_list').hide();

                if (res.is_success) {

                    $('#div_booked_table').html('');
                    var tableDataFrnt = '';
                    tableDataFrnt = '<table id="datatable" class="table table-striped table-bordered">' +
                        '<thead>' +
                        '<tr>' +
                        '<th>#</th>' +
                        '<th>BRANCH_NAME</th>' +
                        '<th>CUSTOMER_NAME</th>' +
                        '<th>BOOKING_DATE</th>' +
                        '<th>CLASS_TIME</th>' +
                        '<th>PROGRAM_NAME</th>' +
                        '</tr>' +
                        '</thead>' +
                        '<tbody>';
                    for (var s = 0; s < res.customer_class_booking_list.length; s++) {
                        var st = res.customer_class_booking_list[s];
                        tableDataFrnt = tableDataFrnt + '<tr>' +
                        '<td>' + (s + 1) + '</td>' +
                        '<td>' + st.branch_name + '</td>' +
                        '<td>' + st.cust_name + '</td>' +
                        '<td>' + st.class_booking_date + '</td>' +
                        '<td>' + st.class_timing + '</td>' +
                        '<td>' + st.class_program_name + '</td>' +
                    '</tr>';

                }
                    tableDataFrnt = tableDataFrnt + '</tbody>' +
                        '</table>'

                    $('#div_booked_table').append(tableDataFrnt);

            } else {
                alert(res.msg);
                //window.location.reload();
            }

        },
        failure: function (data) {
            alert("Network error occurred.");
            $(".overlay").hide();

        },
        error: function (jqXHR, exception) {
            if (jqXHR.status === 0) {
                alert('Please Check Your Internet Connection.');
            } else if (jqXHR.status == 404) {
                alert('Requested page not found. [404]');
            } else if (jqXHR.status == 500) {
                alert('Internal Server Error [500].');
            } else if (exception === 'parsererror') {
                alert('Requested JSON parse failed.');
            } else if (exception === 'timeout') {
                alert('Time out error.');
            } else if (exception === 'abort') {
                alert('Ajax request aborted.');
            } else {
                alert('Uncaught Error.\n' + jqXHR.responseText);
            }
            $(".overlay").hide();
        }

    });
}     //
     //../ProductDetails/ProductDetails?product_id=' + tt.tt_prod_guid + '
    </script>


    <script>
        function showWeekNameAndDate() {

            var week_and_date = "";
            var d = new Date();
            var today_week_number = d.getDay();
            $('#div_weeks_and_date').html('');
            for (var i = 0; i < 7; i++) {

                var next_day_week_number = i + today_week_number;
                console.log("today_week_number: ", today_week_number);
                console.log("next_day_week_number: ", next_day_week_number);
                if (next_day_week_number == 7 || next_day_week_number == 0) {
                    continue;
                }


                if (next_day_week_number >= 8) {
                    next_day_week_number = next_day_week_number - 7;
                }

                var next_date = new Date();
                next_date.setDate(d.getDate() + i);

                var week_name = getWeekName(next_day_week_number);

                var selected_day_id = getWeekName(next_day_week_number);
                var next_date_ = next_date.getDate();
                if (next_date_.length == 1) {
                    next_date_ = "0" + next_date_;
                }

                var params = "'" + week_name + "','" + next_date + "'";
                week_and_date = week_and_date +
                    '<div class="col-2 col-2 col-md-2 col-lg-2" onclick="getTimeTable(' + params + ');">' +
                    '<label>' +
                    '<span >' + week_name + '</span>' +
                    '<center id="' + selected_day_id + '">' + next_date_ + '</center>' +
                    '</label>' +
                    '</div>';
            }
            $('#div_weeks_and_date').html(week_and_date);

        }
    </script>
    <script>
        function showWeekNameAndDate(endDate) {
            var week_and_date = "";
            var d = new Date();
            var today_week_number = d.getDay();
            $('#div_weeks_and_date').html('');

            // Parse the end date passed to the function
            var endDateObj = new Date(endDate);
            endDateObj.setDate(endDateObj.getDate() + 1); // Add one day

            for (var i = 0; i < 7; i++) {
                var next_day_week_number = i + today_week_number;
                console.log("today_week_number: ", today_week_number);
                console.log("next_day_week_number: ", next_day_week_number);

                if (next_day_week_number == 7 || next_day_week_number == 0) {
                    continue;
                }

                if (next_day_week_number >= 8) {
                    next_day_week_number = next_day_week_number - 7;
                }

                var next_date = new Date();
                next_date.setDate(d.getDate() + i);

                // If next_date exceeds the passed end date, stop the loop
                if (next_date > endDateObj) {
                    break;
                }

                var week_name = getWeekName(next_day_week_number);
                var selected_day_id = getWeekName(next_day_week_number);
                var next_date_ = next_date.getDate();

                if (next_date_.length == 1) {
                    next_date_ = "0" + next_date_;
                }

                var params = "'" + week_name + "','" + next_date + "'";
                week_and_date = week_and_date +
                    '<div class="col-2 col-2 col-md-2 col-lg-2" onclick="getTimeTable(' + params + ');">' +
                    '<label>' +
                    '<span >' + week_name + '</span>' +
                    '<center id="' + selected_day_id + '">' + next_date_ + '</center>' +
                    '</label>' +
                    '</div>';
            }

            $('#div_weeks_and_date').html(week_and_date);
        }

    </script>
    <script>
        function makeDateSelected(selected_week_name) {

            var d = new Date();
            var today_week_number = d.getDay();
            for (var i = 0; i < 7; i++) {
                var next_day_week_number = i + today_week_number;

                if (next_day_week_number == 7 || next_day_week_number == 0) {
                    continue;
                }

                if (next_day_week_number >= 8) {
                    next_day_week_number = next_day_week_number - 7;
                }

                var week_name = getWeekName(next_day_week_number);

                if (selected_week_name == week_name) {
                    document.getElementById(selected_week_name).className = "circle_week";
                } else {
                    document.getElementById(week_name).className = "circle_week_";
                }

            }
        }

    </script>

    <script>
        function getWeekName(week_number) {
            switch (week_number) {
                case 1:
                    return "Mon";
                    break;
                case 2:
                    return "Tue";
                    break;
                case 3:
                    return "Wed";
                    break;
                case 4:
                    return "Thu";
                    break;
                case 5:
                    return "Fri";
                    break;
                case 6:
                    return "Sat";
                    break;
                case 7:
                    return "Sun";
                    break;

            }
        }
    </script>






    <script>
        function getBranches() {

            jQuery.ajax({
                type: "POST",
                url: '/ProductDetails/getBranches',
                contentType: "application/json",
                dataType: 'json',
                data: '',
                success: function (res) {
                    console.log("getBranches output : ", JSON.stringify(res));
                    if (res.is_success) {
                        $('#sel_center').html('');
                        var branches = '<option selected value="">--SELECT CENTER--</option>';

                        for (var i = 0; i < res.branch_list.length; i++) {
                            var branch = res.branch_list[i];
                            branches = branches
                           + '<option value="' + branch.branch_guid + '">' + branch.branch_name + '</option>';

                        }
                        $('#sel_center').html(branches);
                        $('#sel_branch').html(branches);
                        $('#sel_class_center').html(branches);

                    } else {
                        alert(res.msg);
                    }

                },
                failure: function (data) {
                    alert("Network error occurred.");
                    $('#btn_login').prop('disabled', false);
                    $(".overlay").hide();
                },
                error: function (jqXHR, exception) {
                    if (jqXHR.status === 0) {
                        alert('Please Check Your Internet Connection.');
                    } else if (jqXHR.status == 404) {
                        alert('Requested page not found. [404]');
                    } else if (jqXHR.status == 500) {
                        alert('Internal Server Error [500].');
                    } else if (exception === 'parsererror') {
                        alert('Requested JSON parse failed.');
                    } else if (exception === 'timeout') {
                        alert('Time out error.');
                    } else if (exception === 'abort') {
                        alert('Ajax request aborted.');
                    } else {
                        alert('Uncaught Error.\n' + jqXHR.responseText);
                    }
                    $('#btn_login').prop('disabled', false);
                    $(".overlay").hide();
                }

            });
        }

    </script>


