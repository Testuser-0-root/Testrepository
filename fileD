    </script>


    
    <script>



    //REGISTER
    function customerRegister() {

        var name = $('#txt_reg_username').val();
        var mobile_number = $('#txt_reg_mobile').val();
        var email = $('#txt_reg_email').val();
        var password = $('#txt_reg_password').val();
        var refered_by = $('#txt_refered_by').val();



        if (name == "") {
            alert("Please enter name");
            $('#txt_reg_username').focus();
            return;
        }

        if (mobile_number == "") {
            alert("Please enter mobile number");
            $('#txt_reg_mobile').focus();
            return;
        }

        if (mobile_number.length != 10) {
            alert("Please enter valid  mobile number");
            $('#txt_reg_mobile').focus();
            return;
        }

        if (email == "") {
            alert("Please enter email");
            $('#txt_reg_email').focus();
            return;
        }

        if (email != "") {
            var expr = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            if (!expr.test(email)) {
                alert("Please enter valid Email ");
                $('#txt_reg_email').focus();
                return;
            }
        }

        if (password == "") {
            alert("Please enter password");
            $('#txt_reg_password').focus();
            return;
        }

        var Checkbox = document.getElementById("exampleCheck1");
        if (!Checkbox.checked) {
            alert("Please Agree To Our Privacy And Policy!!");
            return;
        }


        var inputJson = {
            name: name,
            email: email,
            mobile_number: mobile_number,
            password: password,
            refered_by: refered_by

        };

        console.log("customerRegister input : ", JSON.stringify(inputJson));
        $('#btn_register').prop('disabled', true);
        jQuery.ajax({
            type: "POST",
            url:  '/FrontHome/customerRegister',
            contentType: "application/json",
            dataType: 'json',
            data: JSON.stringify(inputJson),
            success: function (res) {
                $('#btn_register').prop('disabled', false);
                console.log("customerRegister result : ", JSON.stringify(res));
                if (res.is_success) {
                    // alert(res.msg);

                    window.location = "/CustomerProfile/CustomerProfile";

                } else {
                    alert(res.msg);
                }
            },
            failure: function (data) {
                alert("Network error occurred.");
                $('#btn_register').prop('disabled', false);
                $(".overlay").hide();
            },
            error: function (jqXHR, exception) {
                if (jqXHR.status === 0) {
                    alert('Please Check Your Internet Connection.');
                } else if (jqXHR.status == 404) {
                    alert('Requested page not found. [404]');
                } else if (jqXHR.status == 500) {
                    alert('Internal Server Error [500].');
                } else if (exception === 'parsererror') {
                    alert('Requested JSON parse failed.');
                } else if (exception === 'timeout') {
                    alert('Time out error.');
                } else if (exception === 'abort') {
                    alert('Ajax request aborted.');
                } else {
                    alert('Uncaught Error.\n' + jqXHR.responseText);
                }
                $('#btn_register').prop('disabled', false);
                $(".overlay").hide();
            }

        });

    }

    //LOGIN
    function customerLogin() {

        var username = $('#txt_log_username').val();
        var password = $('#txt_log_userpassword').val();

        if (username == "") {
            alert("Please enter username");
            $('#txt_log_username').focus();
            return;
        }
        if (password == "") {
            alert("Please enter password");
            $('#txt_log_userpassword').focus();
            return;
        }

        var inputJson = {
            username: username,
            password: password

        };

        console.log("customerLogin input : ", JSON.stringify(inputJson));

        $('#btn_login').prop('disabled', true);
        $(".overlay").show();
        jQuery.ajax({
            type: "POST",
            url: '/FrontHome/customerLogin',
            contentType: "application/json",
            dataType: 'json',
            data: JSON.stringify(inputJson),
            success: function (res) {

                $('#btn_login').prop('disabled', false);
                $(".overlay").hide();
                if (res.is_success) {
                    //  alert(res.msg);

                    window.location = "/CustomerProfile/CustomerProfile";

                } else {
                    alert(res.msg);
                }

            },
            failure: function (data) {
                alert("Network error occurred.");
                $('#btn_login').prop('disabled', false);
                $(".overlay").hide();
            },
            error: function (jqXHR, exception) {
                if (jqXHR.status === 0) {
                    alert('Please Check Your Internet Connection.');
                } else if (jqXHR.status == 404) {
                    alert('Requested page not found. [404]');
                } else if (jqXHR.status == 500) {
                    alert('Internal Server Error [500].');
                } else if (exception === 'parsererror') {
                    alert('Requested JSON parse failed.');
                } else if (exception === 'timeout') {
                    alert('Time out error.');
                } else if (exception === 'abort') {
                    alert('Ajax request aborted.');
                } else {
                    alert('Uncaught Error.\n' + jqXHR.responseText);
                }
                $('#btn_login').prop('disabled', false);
                $(".overlay").hide();
            }

        });
    }
    </script>


    <script>
    function checkCustomerLogin() {

        var cust_name = '';

            if (cust_name != '') {
                if (cust_name.length > 8) {
                    cust_name = cust_name.substring(0, 8);
                }
                $('#li_customer_login').html('<a href="/CustomerProfile/CustomerProfile" style="color:#17a2b8" class="nav-link" id="a_menu_sign_in">' + cust_name + '</a>');
                hideFreeTrail();
                hideTrail();
            } else {
                $('#li_customer_login').html('<a href="#" data-toggle="modal" data-target="#modal-login" class="nav-link" id="a_menu_sign_in">Sign&#160;in</a>');
            }

            checkScreenSizeInFrontEnd();

        }

    </script>


    <script>
        function loadInit() {
            checkCustomerLogin();
        }
        loadInit();
    </script>




</body>
</html>







