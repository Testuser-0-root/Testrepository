<script>

    function checkWidth() {
        if (jQuery(window).width() < 500) {


            for (var img = 1; img <= 3; img++) {
                img_id = "img_slider_" + img;
                $('#' + img_id).addClass('slider_img_size_mobile');
            }

            //FOR WHY KIKAHA
            $('#why_kk_text_1').addClass('custom-col-sm-6');
            $('#why_kk_text_2').addClass('custom-col-sm-6');
            $('#why_kk_text_3').addClass('custom-col-sm-6');
            $('#why_kk_text_4').addClass('custom-col-sm-6');

            //======FOR PROGRAMS=====
            for (var i = 0; i < prod_programs_size; i++) {
                var _id = "prod" + i;
                $('#' + _id).addClass('custom-col-sm-6');
            }

            //======FOR PACKAGES=====
            //for (var i = 0; i < prod_packages_size; i++) {
            //    var _id = "prod_pkg" + i;
            //    $('#' + _id).addClass('custom-col-sm-6');
            //}



            $('#kk_text_1').addClass('kiakha_mobile');
            $('#kk_text_2').addClass('kiakha_mobile');
            $('#kk_text_3').addClass('kiakha_mobile');
            $('#kk_text_4').addClass('kiakha_mobile');



            //===========REVIEWS==========
            $('#div_review_1').addClass('custom-col-sm-12');
            $('#div_review_2').addClass('custom-col-sm-12');
            $('#div_review_3').addClass('custom-col-sm-12');
            $('#div_review_4').addClass('custom-col-sm-12');


            //===========FAQ==========
            $('#faq_1').addClass('custom-col-sm-4');
            $('#faq_2').addClass('custom-col-sm-4');
            //$('#faq_3').addClass('custom-col-sm-4');
            $('#faq_mobile').addClass('faq_mobile');

            ////LOCATION
            $('#map_1').addClass('custom-col-sm-4');
            $('#map_2').addClass('custom-col-sm-4');
            $('#map_3').addClass('custom-col-sm-4');

        } else {
            for (var img = 1; img <= 3; img++) {
                img_id = "img_slider_" + img;
                $('#' + img_id).addClass('slider_img_size');
            }



            $('#kk_text_1').addClass('kiakha_web');
            $('#kk_text_2').addClass('kiakha_web');
            $('#kk_text_3').addClass('kiakha_web');
            $('#kk_text_4').addClass('kiakha_web');

            $('#div_programs_products').addClass('margin_for_products');
            $('#div_package_products').addClass('margin_for_products');
            $('#div_reviews').addClass('margin_for_products');
            $('#div_reviews_system').addClass('margin_for_products');
            $('#div_faq').addClass('margin_for_products');
            $('#div_location').addClass('margin_for_products');

            $('#h2_programs').addClass('margin_left_for_headings');
            $('#h2_packages').addClass('margin_left_for_headings');
            $('#h2_reviews').addClass('margin_left_for_headings');
            $('#h2_faq').addClass('margin_left_for_headings');
            $('#h2_location').addClass('margin_left_for_headings');

            $('#sub_programs').addClass('margin_for_sub_headings');

        }

    }

    function showAllFaq() {
        $('#div_faq_col_1').show();
        $('#div_faq_col_2').show();
        $('#a_faq_show_two').show();
        $('#a_faq_show_all').hide();

    }

    function showTwoFaq() {
        $('#div_faq_col_1').hide();
        $('#div_faq_col_2').hide();
        $('#a_faq_show_two').hide();
        $('#a_faq_show_all').show();
    }

</script>



<script>
