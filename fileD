    <script>


        function showBookTheClassPopup(mem_branch_id, prod_guid, start_time, end_time, class_name,date_of_book, tt_guid) {
            
            var branch_guid = mem_branch_id, prod_guid = prod_guid, from_time = from_time;
            var to_time = to_time, class_name = class_name, booking_date = date_of_book;
            var class_booking_time = start_time + " To " + end_time;
            var cust_guid = '';
            var class_is_rated = 'false';
            var rate_value = "";
            var review_class = "";


            var inputJson = {
                class_branch_guid: branch_guid,
                class_customer_guid: cust_guid,
                class_booking_date: booking_date,
                class_timing: class_booking_time,
                class_program_name: class_name,
                class_product_guid: prod_guid,
                class_is_rated: class_is_rated,
                class_rating: rate_value,
                class_reviews: review_class,
            };

            console.log("ClassBooked input : ", JSON.stringify(inputJson));


            $('#btn_book_class').prop('disabled', true);
            $(".overlay").show();
            jQuery.ajax({
                type: "POST",
                url: '/BookTheClass/ActiveMemberBookTheClass',
                contentType: "application/json",
                dataType: 'json',
                data: JSON.stringify(inputJson),
                success: function (res) {
                    $('#btn_submit').prop('disabled', false);
                    console.log("ClassBooked result : ", JSON.stringify(res));
                    if (res.is_success) {
                        //alert(res.msg);
                        window.location = "/BookTheClass/BookTheClass";
                        
                        /*$('#btn_book_class').hide();*/
                    } else {
                        alert(res.msg);
                    }
                },
                error: function (jqXHR, exception) {
                    if (jqXHR.status === 0) {
                        alert('Please Check Your Internet Connection.');
                    } else if (jqXHR.status == 404) {
                        alert('Requested page not found. [404]');
                    } else if (jqXHR.status == 500) {
                        alert('Internal Server Error [500].');
                    } else if (exception === 'parsererror') {
                        alert('Requested JSON parse failed.');
                    } else if (exception === 'timeout') {
                        alert('Time out error.');
                    } else if (exception === 'abort') {
                        alert('Ajax request aborted.');
                    } else {
                        alert('Uncaught Error.\n' + jqXHR.responseText);
                    }
                    $('#btn_login').prop('disabled', false);
                    $(".overlay").hide();
                }
            });

        }

            //alert("ddthebooking");
            


    </script>

    <script>
        function showClassBookedPopupTrail(mem_branch_id, prod_guid, class_name, start_time, end_time, tt_guid, date_of_book) {
            var cust_guid = '';
            if (cust_guid == "") {

                showLogin();
                return;
            }

            var mem_cust_guid = '';
            var inputJson = {
                mem_branch_id: mem_branch_id,
                prod_guid: prod_guid,
                class_name: class_name,
                start_time: start_time,
                end_time: end_time,
                mem_cust_guid: mem_cust_guid,
                tt_guid: tt_guid,
                date_of_book: date_of_book

            };

            console.log("showClassBookedPopup input : ", JSON.stringify(inputJson));
            $('#btn_submit').prop('disabled', true);
            $(".overlay").show();
            jQuery.ajax({
                type: "POST",
                url: '/ProductDetails/BookedPopup',
                contentType: "application/json",
                dataType: 'json',
                data: JSON.stringify(inputJson),
                success: function (res) {
                    $('#btn_submit').prop('disabled', false);
                    console.log("showClassBookedPopup result : ", JSON.stringify(res));
                    if (res.is_success) {

                        //  $('#book_Program').attr('hidden', true);;

                        swal({
                            title: "Class Booked!!!",
                            text: "Your class has been successfully booked!",
                            type: "success",
                            confirmButtonText: "OK"
                        }, function (isConfirm) {
                            if (isConfirm) {
                                window.location = "/FrontHome/FrontHome";
                            }
                        });

                    } else {
                        if (!res.is_it_free) {
                            $('#a_menu_free_trial').hide();

                            final_prod_id = prod_guid;
                            final_date_ofbook = date_of_book;
                            final_tt_guid = tt_guid;
                            final_amount = "500";
                            console.log("final_prod_id: " + final_prod_id);
                            console.log("final_tt_guid: " + final_tt_guid);

                            //initiatePayment();
                            alert(res.msg);
                            window.location.reload();
                        }

                    }

                },
                failure: function (data) {
                    alert("Network error occurred.");
                    $('#btn_submit').prop('disabled', false);
                    $(".overlay").hide();
                },
                error: function (jqXHR, exception) {
                    if (jqXHR.status === 0) {
                        alert('Please Check Your Internet Connection.');
                    } else if (jqXHR.status == 404) {
                        alert('Requested page not found. [404]');
                    } else if (jqXHR.status == 500) {
                        alert('Internal Server Error [500].');
                    } else if (exception === 'parsererror') {
                        alert('Requested JSON parse failed.');
                    } else if (exception === 'timeout') {
                        alert('Time out error.');
                    } else if (exception === 'abort') {
                        alert('Ajax request aborted.');
                    } else {
                        alert('Uncaught Error.\n' + jqXHR.responseText);
                    }
                    $('#btn_login').prop('disabled', false);
                    $(".overlay").hide();
                }

            });


        }

    </script>


